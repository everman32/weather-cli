{
  "name": "weather-cli-pro",
  "version": "1.0.0",
  "description": "A console application that provides city weather data",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "weather": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src/**/* --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prit": "prettier --check src/**/*.js",
    "prit:fix": "prettier --write src/**/*.js",
    "prepare": "husky install"
  },
  "keywords": [
    "weather",
    "cli",
    "nodejs"
  ],
  "author": "Victor Borodyn",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "commander": "^9.4.0"
  }
}
