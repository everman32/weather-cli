{
  "name": "weather-cli-pro",
  "version": "1.0.0",
  "description": "A console application that provides city weather data",
  "config": {
    "startPointPath": "src/index.js",
    "srcPath": "src/**/*",
    "srcExtensions": ".js"
  },
  "main": "%npm_package_config_startPointPath%",
  "type": "module",
  "bin": {
    "weather": "%npm_package_config_startPointPath%"
  },
  "scripts": {
    "start": "node %npm_package_config_startPointPath%",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint %npm_package_config_srcPath% --ext %npm_package_config_srcExtensions%",
    "lint:fix": "eslint %npm_package_config_srcPath% --ext %npm_package_config_srcExtensions% --fix",
    "prit": "prettier --write %npm_package_config_srcPath%%npm_package_config_srcExtensions%",
    "prepare": "husky install"
  },
  "keywords": [
    "weather",
    "cli",
    "nodejs"
  ],
  "author": "Victor Borodyn",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "commander": "^9.4.0"
  }
}
